{
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.995.0",
    "@aws-sdk/client-dynamodb": "^3.995.0",
    "@aws-sdk/client-s3": "^3.995.0",
    "@aws-sdk/client-sns": "^3.995.0",
    "@aws-sdk/lib-dynamodb": "^3.995.0"
  },
  "name": "stock-analyzer",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "eslint": "^10.0.1",
    "prettier": "^3.8.1"
  },
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --test",
    "ci": "npm run format:check && npm run lint && npm run test",
    "sam:validate": "sam validate --template-file infra/template.yaml",
    "sam:build": "sam build --template-file infra/template.yaml",
    "sam:sync": "sam sync --template-file infra/template.yaml --watch",
    "deploy": "sam deploy --template-file infra/template.yaml",
    "deploy:guided": "sam deploy --guided --template-file infra/template.yaml",
    "delete": "sam delete",
    "local:perTicker": "sam local invoke PerTickerFunction --template-file infra/template.yaml --event infra/events/perTicker.json",
    "local:buildAlert": "sam local invoke BuildAlertFunction --template-file infra/template.yaml --event infra/events/buildAlert.json",
    "local:runDaily": "sam local invoke RunDailyFunction --template-file infra/template.yaml --event infra/events/runDaily.json",
    "cloud:run": "node ./tools/start-execution.mjs",
    "cloud:run:today": "node ./tools/start-execution.mjs --today",
    "cloud:run:tickers": "node ./tools/start-execution.mjs --tickers AAPL,MSFT,AMZN,GOOGL,NVDA,META,TSLA,BRK.B,JPM,XOM",
    "logs:perTicker": "sam logs -n PerTickerFunction --template-file infra/template.yaml --tail",
    "logs:buildAlert": "sam logs -n BuildAlertFunction --template-file infra/template.yaml --tail",
    "logs:runDaily": "sam logs -n RunDailyFunction --template-file infra/template.yaml --tail",
    "clean": "rm -rf .aws-sam node_modules/.cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
